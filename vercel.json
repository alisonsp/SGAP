{
    "builds": [
      {
        "src": "sgap/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "15mb",
          "runtime": "Python3.13"
        },
        {
          "src": "build_files.sh",
          "use": "@vercel/static-build",
          "config": {
            "distDir": "ui/staticfiles"
          }
        }
      }],
    "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "sgap/wsgi.py"
    }
  ],
  "outputDirectory": "ui/staticfiles"
}
    